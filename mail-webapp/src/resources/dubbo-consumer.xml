<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="ceb-admin-webapp" />

    <!-- dubbo客户端统一配置 -->
    <dubbo:consumer timeout="60000" retries="0"/>

    <!-- 使用ZooKeeper广播注册中心 -->
    <dubbo:registry protocol="zookeeper" address="${zk.address}" file="${user.home}/dubbo/cache/ceb-admin-webapp.cache"/>

    <!-- 数据字典服务 -->
    <dubbo:reference id="dictionaryClient" interface="com.ceair.ceb.basicdata.service.DictionaryService" validation="true" version="1.0.0"/>


</beans>